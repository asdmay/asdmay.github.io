{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "4.0.0",
    "ress": "^1.2.2",
    "styled-components": "^5.2.1"
  },
  "homepage": "http://asdmay.github.io",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "build-css": "sass --no-source-map src:src",
    "watch-css": "npm run build-css && sass --watch src:src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "react-responsive": "^8.1.1",
    "sass": "^1.94.0"
  }
}
